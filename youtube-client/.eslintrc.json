{
  "extends": "airbnb-base",
  "rules": {
    "generators": "never",
    "wrap-iife": [2, "any"],
    "name": "never",
    "max-len": ["error", { "code": 150 }],
    "no-shadow": "off",
    "no-param-reassign": 0,
    "no-use-before-define": ["error", { "variables": false }],
    "no-unused-expressions": ["error", {"allowTernary": true}],
    "linebreak-style": 0,
    "no-plusplus": "off",
    "prefer-destructuring": ["error", {
      "array": false,
      "object": false
    }, {
      "enforceForRenamedProperties": false
    }]
  },
  "env": {
    "browser": true
  }
}